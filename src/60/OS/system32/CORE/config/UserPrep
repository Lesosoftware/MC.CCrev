_G["Users"]={}
_G["Users"]["col"]={}

_G["um"]={}
_G["um"]["a"] = function(group,nUsr)
  if _G.Users[group]==nil then
    _G.Users[group]={}
  end
  _G.Users[group][#_G.Users[group]+1]=nUsr
end
_G["um"]["sCol"] = function(group,nCol)
  if _G.Users[group]==nil then
    _G.Users[group]={}
  end
  _G.Users.col[group]=nCol
end
